package com.projeto3.model;

import java.util.List;
import java.util.Map;

public class Feedback {
    private String surveyId;
    private String surveyTitle;
    private double responseRate;
    private int totalParticipants;
    private String averageTime;
    private List<Map<String, Object>> barChartData;
    private List<Map<String, Object>> pieChartData;

    
    public Feedback() {}

    public Feedback(String surveyId, String surveyTitle, double responseRate, 
                   int totalParticipants, String averageTime) {
        this.surveyId = surveyId;
        this.surveyTitle = surveyTitle;
        this.responseRate = responseRate;
        this.totalParticipants = totalParticipants;
        this.averageTime = averageTime;
    }

    
    public String getSurveyId() { return surveyId; }
    public void setSurveyId(String surveyId) { this.surveyId = surveyId; }

    public String getSurveyTitle() { return surveyTitle; }
    public void setSurveyTitle(String surveyTitle) { this.surveyTitle = surveyTitle; }

    public double getResponseRate() { return responseRate; }
    public void setResponseRate(double responseRate) { this.responseRate = responseRate; }

    public int getTotalParticipants() { return totalParticipants; }
    public void setTotalParticipants(int totalParticipants) { this.totalParticipants = totalParticipants; }

    public String getAverageTime() { return averageTime; }
    public void setAverageTime(String averageTime) { this.averageTime = averageTime; }

    public List<Map<String, Object>> getBarChartData() { return barChartData; }
    public void setBarChartData(List<Map<String, Object>> barChartData) { this.barChartData = barChartData; }

    public List<Map<String, Object>> getPieChartData() { return pieChartData; }
    public void setPieChartData(List<Map<String, Object>> pieChartData) { this.pieChartData = pieChartData; }
}
